    "graphql-cli": "^4.0.0",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "paystack": "^2.0.1",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "react-apollo": "^3.1.5"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tunedev/Online_shop_backend.git"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "apollo-client": "^2.6.10",
